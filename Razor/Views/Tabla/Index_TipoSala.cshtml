@model List<Razor.Models.ViewModels.ListTablaViewModelTipoSala>
@{
    ViewBag.Title = "Tipo_Sala_Juntas";
}

<h2>@ViewBag.Title</h2>

<div class="row" style="text-align:right;padding:5px;">
    <a href="~/Tabla/Nueva_TipoSala" class=" btn btn-primary">Tipo Sala Juntas</a>
    <div class="col-md-12">
        <table class="table">
            <tr>
                <th>IdTipoSala</th>
                <th>TipoSalaJuntas</th>
                

            </tr>
            @foreach (var p in Model)
            {
                <tr>
                    <td>@p.IdTipoSala</td>
                    <td>@p.TipoSalaJuntas</td>
                    

                   
                </tr>
            }

        </table>
       

    </div>

</div>